{
  "compilerOptions": {
    "target": "ES6",
    "module": "es6",
    "lib": ["es6", "dom"],
    "moduleResolution": "node",
    "noEmit": true,
    "downlevelIteration": true,
    "allowJs": true,
    "esModuleInterop": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./*.ts", "./*.js"],
  "exclude": ["node_modules"]
}
